{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "oswee/*": [
        "*"
      ]
    },
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES6",
    "sourceMap": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "bazel-*",
    "node_modules"
  ]
}
